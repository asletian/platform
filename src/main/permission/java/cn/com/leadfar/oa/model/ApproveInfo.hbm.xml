<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.com.leadfar.oa.model.ApproveInfo" table="t_approveinfo">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="approveTime"/>
		<property name="comment"/>
		<many-to-one name="approver"></many-to-one>
		<many-to-one name="leaveForm"></many-to-one>
	</class>
</hibernate-mapping>