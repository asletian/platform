<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.com.leadfar.oa.model.EntityProperty" table="t_entityproperties">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="fileValue" type="binary" length="99999999" access="field"/>
		<property name="dateValue" access="field"/>
		<property name="intValue" access="field"/>
		<property name="stringValue" access="field"/>
		<property name="doubleValue" access="field"/>
		<property name="longValue" access="field"/>
		<property name="propertyType"/>
		<property name="fileValueFileName"/>
		<property name="fileValueContentType"/>
	</class>
</hibernate-mapping>